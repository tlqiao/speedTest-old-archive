{
  "request": {
    "method": "GET",
    "urlPath": "/search",
    "queryParameters": {
      "username": {
        "matches": "zhang[a-z]+"
      },
      "sex": {
        "equalTo": "female"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "userDetails.json",
    "headers": {
      "Content-Type": "application/json; charset=UTF-8"
    }
  }
}
